{
  "name": "imooc-on-blockchain",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.15.0",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.0",
    "bignumber.js": "^8.1.1",
    "customize-cra": "^0.2.12",
    "ganache-cli": "^6.3.0",
    "ipfs-http-client": "^30.1.0",
    "mocha": "^6.0.2",
    "onchange": "^5.2.0",
    "react": "^16.8.3",
    "react-app-rewired": "^2.1.1",
    "react-dom": "^16.8.3",
    "react-keep-alive": "^1.2.0",
    "react-router-cache-route": "^1.4.2",
    "react-router-dom": "^4.4.0",
    "react-scripts": "2.1.5",
    "rimraf": "^2.6.3",
    "solc": "^0.5.0",
    "truffle-hdwallet-provider": "^1.0.5",
    "web3": "^1.0.0-beta.52"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "deploy": "node scripts/deploy.js",
    "precompile": "rimraf src/compiled/*",
    "compile": "node scripts/compile.js",
    "precompile:w": "npm run compile",
    "compile:w": "onchange \"contracts/*.sol\" -- npm run compile",
    "test:w": "npm run compile && mocha --watch",
    "rebuild": "npm run compile && mocha",
    "prerebuild:w": "npm run rebuild",
    "rebuild:w": "onchange \"contracts/*.sol\" \"test/*.js\" -- npm run rebuild",
    "mocha": "mocha --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "mocha": {
    "timeout": 3000
  }
}
